{
  "name": "SoundCloud Stats",
  "version": "1.1",
  "description": "SoundCloud stats",
  "browser_action": {
      "default_title": "SoundCloud Stats",
      "default_icon": "icon.png",
      "default_popup": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.soundcloud.com; object-src 'self'"
}
